<Window x:Class="VapeTeam.Psimulex.Graphics.Windows.InputBox"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="InputBox" Height="143" Width="375" SnapsToDevicePixels="True" WindowStartupLocation="CenterOwner" ShowInTaskbar="False" Loaded="Window_Loaded">
    <Grid>
        <Label Height="20" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" Width="120" Padding="2">Enter the value:</Label>
        <TextBox Margin="11,38,12,0" Name="textBox1" Height="25" VerticalAlignment="Top" />
        <Button Height="27" Margin="0,69,12,0" Name="okButton" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" Click="okButton_Click" IsDefault="True">OK</Button>
    </Grid>
</Window>
