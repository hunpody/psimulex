<Window x:Class="VapeTeam.Psimulex.Compiler.Antlr.WpfTest.SyntaxTreeWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="SyntaxTreeWindow" Height="371" Width="592" Loaded="Window_Loaded">
    <Grid>
        <Menu Height="22" Name="menu1" VerticalAlignment="Top" >
        <MenuItem Name="File" Header="File" FlowDirection="LeftToRight" HorizontalContentAlignment="Left" HorizontalAlignment="Stretch">
                <MenuItem Name="Hibrid" Header="Hibrid" Click="Hibrid_Click"/>
                <MenuItem Name="Value" Header="Value" Click="Value_Click"/>
                <MenuItem Name="AllVisible" Header="AllVisible" Click="AllVisible_Click"/>
                <MenuItem Name="Everything" Header="Everything" Click="Everything_Click"/>
                <MenuItem Name="Interval" Header="Interval" Click="Interval_Click"/>
                    <Separator />
                <MenuItem Name="Configure" Header="Configure" Click="Configure_Click"/>
                    <Separator />
                <MenuItem Name="Exit" Header="Exit" Click="Exit_Click"/>
            </MenuItem>
        </Menu>
        <TreeView Margin="0,22,0,0" Name="TreeView" />
    </Grid>
</Window>
