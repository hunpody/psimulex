int i = 0;
while( i < 10 )
{
	print(i);
	++i;
}

print("\n");

int i = 0;
do
{
	print(i);
	i++;
}while( i < 10 );

print("\n");

for( int i = 0; i < 10; i++ )
	print(i);

print("\n");

loop( int i = 0 to 9 )
	print(i);
	
list L;
for(int i = 0; i < 10; ++i)
	L.Insert(i);

foreach( int i in L )
	print(i);

print("\n");

iterator it=L.getiterator();
while(it.movenext())
{
	int i=it.current();
	print(i);
}